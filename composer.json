{
    "name": "jonas-elias/hyperf-oracle",
    "type": "library",
    "description": "A oracle handler for hyperf/database.",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "database",
        "oracle",
        "oci8"
    ],
    "require": {
        "php": ">=8.0",
        "ext-swoole": ">=5.0",
        "hyperf/collection": "~3.0.0",
        "hyperf/database": "~3.0.0",
        "hyperf/support": "~3.0.0",
        "hyperf/tappable": "~3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\Database\\Oracle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Database\\Oracle\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Database\\Oracle\\ConfigProvider"
        }
    }
}
